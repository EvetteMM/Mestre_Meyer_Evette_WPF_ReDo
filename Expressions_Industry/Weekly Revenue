<html xmlns="http://www.w3.org/1999/xhtml">
<body>
<script type="text/javascript">
//Calculate how much each designer generates in revenue each week by how much they bill per hour and how many hours they bill for.

var kateWebHoursBilled = prompt("How many hours did Kate bill for web design this week?");
var jaySocMedHoursBilled = prompt("How many hours did Jay bill for social media this week?");
var saraEmailMktgHoursBilled = prompt("How many hours did Sara bill for email marketing this week?");
var lisaGraphicHoursBilled = prompt("How many hours did Lisa bill for graphic design this week?");
//show hourly rate each team member charges
var kateHourlyRate = 50;
var jayHourlyRate = 40;
var saraHourlyRate = 30;
var lisaHourlyRate = 25;
//multiply hours billed by hourly rate for each team member; then add them all together
var revenueForWeek = (kateWebHoursBilled * kateHourlyRate) + (jaySocMedHoursBilled * jayHourlyRate) + (saraEmailMktgHoursBilled * saraHourlyRate) + (lisaGraphicHoursBilled * lisaHourlyRate);
alert("Our team generated $" + revenueForWeek + " this week!")
</script>
</body>
